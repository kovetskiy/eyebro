{
  "name": "Eye Bro",
  "description": "client for eyebrod",
  "version": "3.0",
  "permissions": [
    "tabs",
    "activeTab",
    "webSocketPermissionRequest"
  ],
  "host_permissions": [
    "ws://localhost:3451/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3,
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ]
}
